<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.administrator.shopping.activity.MyOrderActivity">
    <RelativeLayout
        android:padding="10dp"
        android:background="@color/redFive"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:padding="5dp"
            android:id="@+id/iv_my_order_back"
            android:layout_margin="10dp"
            android:layout_width="wrap_content"
            android:src="@mipmap/back"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="wrap_content"
            android:text="我的订单"
            android:layout_centerInParent="true"
            android:textSize="@dimen/text_size_one"
            android:textColor="@color/white"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <android.support.design.widget.TabLayout
        android:id="@+id/tabs"
        android:background="#F1F1F1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tabIndicatorColor="@color/redFive"
        app:tabSelectedTextColor="@color/redFive"
        app:tabTextColor="@color/black">
    </android.support.design.widget.TabLayout>
      <android.support.v4.view.ViewPager
      android:id="@+id/viewpager"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">
     </android.support.v4.view.ViewPager>
     </LinearLayout>
